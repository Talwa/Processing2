void setup() {
  size(500, 500);
  background(#0B0643);
}

int value = #454546;

void draw() {
  fill(#F0E96A);
  ellipse(450, 60, 60, 60);
  fill(#050931);
  rect(0, 200, 125, 300);
  rect(125, 175, 125, 325);
  rect(250, 225, 125, 275);
  rect(375, 200, 125, 300); 
  fill(value);
  rect(25, 250, 25, 50);
  rect(75, 325, 25, 50);
  rect(25, 400, 25, 50);
  rect(75, 250, 25, 50);
  rect(25, 325, 25, 50);
  rect(75, 400, 25, 50);
  rect(150, 300, 25, 50);
  rect(200, 300, 25, 50);
  rect(150, 375, 25, 50);
  rect(200, 375, 25, 50);
  rect(150, 225, 25, 50);
  rect(200, 225, 25, 50);
  rect(275, 275, 25, 50);
  rect(325, 275, 25, 50);
  rect(275, 350, 25, 50);
  rect(325, 350, 25, 50);
  rect(275, 425, 25, 50);
  rect(325, 425, 25, 50);
  rect(400, 250, 25, 50);
  rect(400, 325, 25, 50);
  rect(400, 400, 25, 50);
  rect(450, 250, 25, 50);
  rect(450, 325, 25, 50);
  rect(450, 400, 25, 50);

}

void mouseClicked() {
  if (value == #454546) {
    value = #F0E96A;
  } else {
    value = #454546;
  }
}
  